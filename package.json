{
  "name": "React-Boilerplate",
  "version": "1.0.0",
  "description": "React-Boilerplate",
  "main": "app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "server": "./node_modules/.bin/http-server ./www",
    "buildjsb": "./node_modules/.bin/browserify -t [ babelify --presets [ es2015 react ] ] -e js/app.js -o www/bundle.js",
    "buildjs": "./node_modules/.bin/browserify -t [ babelify --presets [ es2015 react ] ] -g uglifyify -e js/app.js -o www/bundle.js",
    "watchjs": "./node_modules/.bin/watchify -t [ babelify --presets [ es2015 react ] ] -g uglifyify -e js/app.js -o www/bundle.js",
    "buildcss": "sass --update sass/app.scss:www/bundle.css --style compressed",
    "watchcss": "nodemon -e scss --watch sass/ -x 'npm run buildcss'",
    "watch": "npm run watchjs | npm run watchcss"
  },
  "author": "John Salter",
  "license": "ISC",
  "dependencies": {
    "gsap": "^1.19.0",
    "howler": "^2.0.1",
    "jquery": "^2.2.2",
    "lodash": "^4.15.0",
    "preload-js": "^0.6.3",
    "react": "^15.6.1",
    "react-addons-css-transition-group": "^15.6.0",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.2.2",
    "react-transition-group": "^1.2.0",
    "redux": "^3.7.2"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.2",
    "babel-core": "^6.22.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.24.1",
    "babel-runtime": "^6.22.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.0",
    "es6-promise": "^4.0.5",
    "http-server": "^0.9.0",
    "jest": "^20.0.4",
    "postcss-cli": "^2.6.0",
    "redux-devtools": "^3.4.0",
    "uglifyify": "^4.0.3",
    "watchify": "^3.7.0"
  }
}
